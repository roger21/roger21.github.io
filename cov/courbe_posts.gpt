set y2label "{/=16 le nombre de posts roses"
set key at 1579608000, 140000 center left Left opaque reverse samplen 3 box spacing 1.3 width -3.1 height .3
set xtics 1579478400, 604800, 1585526400
set xtics add ("lun 20/01/2020" 1579478400, "lun 27/01/2020" 1580083200, "lun 03/02/2020" 1580688000, "lun 10/02/2020" 1581292800, "lun 17/02/2020" 1581897600, "lun 24/02/2020" 1582502400, "lun 02/03/2020" 1583107200, "lun 09/03/2020" 1583712000, "lun 16/03/2020" 1584316800, "lun 23/03/2020" 1584921600, "lun 30/03/2020" 1585526400)
