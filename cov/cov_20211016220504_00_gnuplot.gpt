

# Le nombre de posts et de participants du topic COVID-19

unset multiplot
reset session
reset
set encoding utf8
set terminal svg size 1920, 1080 dynamic mouse standalone name "cov_20211016220504_01_counts" font "Verdana,Arial,Sans-serif,Helvetica,14" dashlength 2 linewidth 1 background "#ffffff"
set output "cov_20211016220504_01_counts.svg"
set title "{/:Bold=18 Le nombre de posts et de participants du topic COVID-19\n{/:Bold=14 (données collectées le samedi 16 octobre 2021 à 22:05:04)"
set ylabel "{/=16 le nombre de posts"
set y2label "{/=16 le nombre de posts roses et de participants"
set key at graph .025, graph .96 top left Left opaque reverse samplen 3 box spacing 1.2 height .4 width -7
set xtics ("dec 2019" 1575158400, "jan 2020" 1577836800, "feb 2020" 1580515200, "mar 2020" 1583020800, "apr 2020" 1585699200, "may 2020" 1588291200, "jun 2020" 1590969600, "jul 2020" 1593561600, "aug 2020" 1596240000, "sep 2020" 1598918400, "oct 2020" 1601510400, "nov 2020" 1604188800, "dec 2020" 1606780800, "jan 2021" 1609459200, "feb 2021" 1612137600, "mar 2021" 1614556800, "apr 2021" 1617235200, "may 2021" 1619827200, "jun 2021" 1622505600, "jul 2021" 1625097600, "aug 2021" 1627776000, "sep 2021" 1630454400, "oct 2021" 1633046400, "nov 2021" 1635724800, "dec 2021" 1638316800) rotate
set ytics 0, 200000
set mytics 2
set y2tics 0, 1000
set link y2 via y / 200 inverse y * 200
set grid xtics mxtics ytics mytics back lc "#3f3f3f", lc "#9f9f9f"
set format y "%.0f"
set format y2 "%.0f"
set xdata time
set timefmt "%s"
set format x "%F %T"
set mouse mouseformat "DateTime"

set size .985,1
set origin .015,0

bigger(s) = sprintf("{/:Bold=16 %.f", s)

plot [1577836800:1635724800][0:1000000] \
"cov_20211016220504_00_data_count_posts.txt" u 1:2 w line lw 2 lc rgbcolor "#ff7f00" title "le nombre de posts", \
"cov_20211016220504_00_data_count_roses.txt" u 1:2 axes x1y2 w line lw 2 lc rgbcolor "#3faf3f" title "le nombre de posts roses (sur y2)", \
"cov_20211016220504_00_data_count_participants.txt" u 1:2 axes x1y2 w line lw 2 lc rgbcolor "#7f5fff" title "le nombre de participants (sur y2)", \
"cov_20211016220504_00_data_count_posts.txt" every ::857150::857150 u 1:2:(bigger($2)) w labels right tc rgbcolor "#ff7f00" point lc rgbcolor "#ff7f00" pt 7 ps 1 offset 0,.6 notitle, \
"cov_20211016220504_00_data_count_roses.txt" every ::866::866 u 1:2:(bigger($2)) axes x1y2 w labels right tc rgbcolor "#3faf3f" point lc rgbcolor "#3faf3f" pt 7 ps 1 offset 0,.6 notitle, \
"cov_20211016220504_00_data_count_participants.txt" every ::3291::3291 u 1:2:(bigger($2)) axes x1y2 w labels right tc rgbcolor "#7f5fff" point lc rgbcolor "#7f5fff" pt 7 ps 1 offset 0,.6 notitle

set output


# L'évolution du nombre de participants du topic COVID-19

unset multiplot
reset session
reset
set encoding utf8
set terminal svg size 1920, 1080 dynamic mouse standalone name "cov_20211016220504_02_evol" font "Verdana,Arial,Sans-serif,Helvetica,14" dashlength 2 linewidth 1 background "#ffffff"
set output "cov_20211016220504_02_evol.svg"
set multiplot title "{/:Bold=18 L'évolution du nombre de participants du topic COVID-19\n{/:Bold=14 (données collectées le samedi 16 octobre 2021 à 22:05:04)"
set xtics ("dec 2019" 1575158400, "jan 2020" 1577836800, "feb 2020" 1580515200, "mar 2020" 1583020800, "apr 2020" 1585699200, "may 2020" 1588291200, "jun 2020" 1590969600, "jul 2020" 1593561600, "aug 2020" 1596240000, "sep 2020" 1598918400, "oct 2020" 1601510400, "nov 2020" 1604188800, "dec 2020" 1606780800, "jan 2021" 1609459200, "feb 2021" 1612137600, "mar 2021" 1614556800, "apr 2021" 1617235200, "may 2021" 1619827200, "jun 2021" 1622505600, "jul 2021" 1625097600, "aug 2021" 1627776000, "sep 2021" 1630454400, "oct 2021" 1633046400, "nov 2021" 1635724800, "dec 2021" 1638316800) rotate
set ytics 0, 1000
set mytics 2
set grid xtics mxtics ytics mytics back lc "#3f3f3f", lc "#9f9f9f"
set style fill transparent solid .4
set format y "%.0f"
set format y2 "%.0f"
set xdata time
set timefmt "%s"
set format x "%F %T"
set mouse mouseformat "DateTime"

set ylabel "{/=16 le nombre de participants"
set key at graph .975, first 250 bottom right Left opaque reverse samplen 3 box spacing 1.2 height .4 width -7

# il faut déterminer k empiriquement, ici k vaut .11
# ensuite on peut choisir la taille des graphs et du titre avec la formule t + 2x + k + x + k = 1
# je donne .06 pour t et k vaut .11 donc x = (1 - .06 - 2 * .11) / 3 = .72 / 3 = .24
set size .985,.59 # 2x + k
set origin .015,.35 # x + k

plot [1577836800:1635724800][0:4000] \
"cov_20211016220504_00_data_evol.txt" u 1:2 w lines lw 2 lc rgbcolor "#003fbf" title "le nombre de premières participations", \
"cov_20211016220504_00_data_evol.txt" u 1:3 w lines lw 2 lc rgbcolor "#bf3f3f" title "le nombre de dernières participations", \
"cov_20211016220504_00_data_evol.txt" u 1:2:3 w filledcurves lw 2 lc rgbcolor "#7f3fff" title "le nombre de participants présents   "

set ylabel "{/=16 le nombre de participants"
set key at graph .975, first 1750 top right Left opaque reverse samplen 3 box spacing 1.2 height .4 width -7

set size .985,.35 # x + k
set origin .015,0

plot [1577836800:1635724800][0:2000] \
"cov_20211016220504_00_data_evol.txt" u 1:4 smooth bezier lw 2 lc rgbcolor "#7f3fff" title "le nombre de participants présents   "

unset multiplot
set output


